    <script>
        // ===== SISTEMA DE TRACKING COM WEBHOOK =====
        const WEBHOOK_URL = 'https://hook.us2.make.com/md8tzkcsr6wxwkz8toctsy2la2ob5s1f';
        
        // Vari√°vel global para armazenar o nome do lead ap√≥s captura
        let nomeLeadCapturado = '';

        // Debug: Verificar se modal existe ao carregar
        console.log('Script carregado');
        document.addEventListener('DOMContentLoaded', function() {
            const modal = document.getElementById('modalLojas');
            console.log('Modal encontrado:', modal ? 'SIM' : 'N√ÉO');
        });
        
        async function enviarWebhook(dados) {
            try {
                // Inclui o nome capturado e identificador da p√°gina em todos os webhooks
                const dadosCompletos = {
                    ...dados,
                    nome: dados.nome || nomeLeadCapturado || '',
                    fonte: 'LP Degusta√ß√£o Queijos Frios'
                };
                
                const response = await fetch(WEBHOOK_URL, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(dadosCompletos)
                });
                
                console.log('Tracking enviado:', dadosCompletos);
                return response.ok;
            } catch (error) {
                console.error('Erro ao enviar tracking:', error);
                return false;
            }
        }

        // ===== HERO BACKGROUND SLIDER (2 IMAGENS) =====
        let heroCurrentIndex = 0;
        const heroItems = document.querySelectorAll('.hero-bg-item');
        const heroTotalItems = heroItems.length;

        function changeHeroBackground() {
            heroItems[heroCurrentIndex].classList.remove('active');
            heroCurrentIndex = (heroCurrentIndex + 1) % heroTotalItems;
            heroItems[heroCurrentIndex].classList.add('active');
        }

        setInterval(changeHeroBackground, 20000);

        function scrollToProdutos() {
            document.getElementById('produtos').scrollIntoView({ behavior: 'smooth' });
        }

        function scrollToFooter() {
            const sec = document.getElementById('localizacoes-bancas');
            if (sec) {
                sec.scrollIntoView({ behavior: 'smooth' });
            } else {
                const modal = document.getElementById('modalLojas');
                if (modal) {
                    modal.style.display = 'block';
                }
            }
        }

        function fecharModal(modalId) {
            const modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = 'none';
            }
        }

        // Fechar modal ao clicar fora
        window.onclick = function(event) {
            const modal = document.getElementById('modalLojas');
            if (event.target === modal) {
                modal.style.display = 'none';
            }
        }

        function scrollMarquee(marqueeId, direction) {
            const container = document.getElementById(marqueeId);
            const scrollAmount = 400; // pixels para rolar
            const currentScroll = container.scrollLeft;
            
            container.scrollTo({
                left: currentScroll + (scrollAmount * direction),
                behavior: 'smooth'
            });
        }

        // FUN√á√ïES DE INTERA√á√ÉO COM TRACKING
        function abrirWhatsApp(produto, secao = 'produtos') {
            abrirCapturaWhatsApp(produto, secao);
        }

        // VARI√ÅVEIS PARA CAPTURA
        let capturaAcao = 'whatsapp';
        let capturaProduto = '';

        // ABRIR MODAL DE CAPTURA PARA WHATSAPP
        function abrirCapturaWhatsApp(produto, secao = 'produtos') {
            capturaAcao = 'whatsapp';
            capturaProduto = produto;
            
            document.getElementById('capturaSubtitulo').textContent = 'Agendar Degusta√ß√£o Cortesia';
            document.getElementById('capturaAviso').textContent = '‚úÖ Digite seu nome e WhatsApp para seguirmos com seu atendimento';
            
            document.getElementById('modalCapturaNome').style.display = 'flex';
            document.getElementById('nomeCaptura').focus();
        }

        // FECHAR MODAL DE CAPTURA
        function fecharCaptura() {
            document.getElementById('modalCapturaNome').style.display = 'none';
            document.getElementById('nomeCaptura').value = '';
            const tel = document.getElementById('telefoneCaptura');
            if (tel) tel.value = '';
            const sucesso = document.getElementById('capturaSucesso');
            if (sucesso) sucesso.style.display = 'none';
            const submitBtn = document.querySelector('.modal-btn-submit');
            if (submitBtn) {
                submitBtn.disabled = false;
                submitBtn.textContent = 'Continuar ‚Üí';
            }
            capturaAcao = '';
            capturaProduto = '';
        }

        // FECHAR MODAL GEN√âRICO
        function fecharModal(modalId) {
            const modal = document.getElementById(modalId);
            if (modal) {
                modal.style.display = 'none';
                console.log('Modal fechado');
            }
        }

        // ENVIAR CAPTURA
        function enviarCaptura() {
            const nome = document.getElementById('nomeCaptura').value.trim();
            const telefone = document.getElementById('telefoneCaptura').value.trim();
            
            if (!nome) {
                alert('Por favor, digite seu nome para continuar.');
                return;
            }
            if (!telefone) {
                alert('Por favor, informe seu WhatsApp (com DDD).');
                return;
            }

            // Envia evento Lead para Meta Pixel
            if (typeof fbq !== 'undefined') {
                fbq('track', 'Lead', {
                    content_name: capturaProduto,
                    content_category: 'degustacao_gratuita',
                    value: 0,
                    currency: 'BRL'
                });
            }

            // Armazena o nome globalmente para futuras intera√ß√µes
            nomeLeadCapturado = nome;

            // Envia tracking com nome e telefone
            enviarWebhook({
                nome: nome,
                telefone: telefone,
                origem: 'captura_modal',
                tipo_acao: 'whatsapp_degustacao',
                produto: capturaProduto,
                secao: 'modal_captura',
                timestamp: new Date().toISOString()
            });

            // Exibe mensagem de sucesso no pr√≥prio modal
            const sucesso = document.getElementById('capturaSucesso');
            if (sucesso) sucesso.style.display = 'block';

            // Desabilita bot√µes para evitar reenvio
            const submitBtn = document.querySelector('.modal-btn-submit');
            if (submitBtn) {
                submitBtn.disabled = true;
                submitBtn.textContent = 'Recebido ‚úÖ';
            }
        }

        // Event listener para Enter no input
        document.addEventListener('DOMContentLoaded', function() {
            const inputCaptura = document.getElementById('nomeCaptura');
            if (inputCaptura) {
                inputCaptura.addEventListener('keypress', function(e) {
                    if (e.key === 'Enter') {
                        enviarCaptura();
                    }
                });
            }
            
            // Renderizar marquees
            renderizarMarquees();
            
            // Renderizar outros produtos
            const gridOutros = document.getElementById('productsGridOutros');
            if (gridOutros) {
                outrosProdutos.forEach(produto => {
                    const card = document.createElement('div');
                    card.className = 'product-card';
                    card.innerHTML = `
                        <div class="product-image-wrapper">
                            <img src="${produto.imagem}" 
                                 alt="${produto.nome} - Banca Pacheco" 
                                 class="product-image"
                                 width="400"
                                 height="240"
                                 loading="lazy">
                            <div class="product-badge">${produto.badge}</div>
                        </div>
                        <div class="product-content">
                            <div class="product-category">¬ª ${produto.categoria}</div>
                            <h3 class="product-title">${produto.nome}</h3>
                            <p class="product-description">${produto.descricao}</p>
                            <button class="product-cta" onclick="abrirWhatsApp('${produto.nome}')">
                                üí¨ Quero Encomendar
                            </button>
                        </div>
                    `;
                    gridOutros.appendChild(card);
                });
            }
        });

        // FUN√á√ÉO PADR√ÉO PARA CONSTRUIR MENSAGEM WHATSAPP
        function montarMensagemWhatsApp(nome = '', contexto = '') {
            return 'Ol√°! Quero agendar uma degusta√ß√£o dos queijos e frios da Banca Pacheco.';
        }

        // ===== PRODUTOS DIN√ÇMICOS =====
        // PRODUTOS PARA MARQUEE DO HERO
        const produtosHeroMarquee = [
            'Copa_Pamplona_Fatiada.jpg',
