        function criarMarqueeItem(produto) {
            return `
                <div class="marquee-item" onclick="abrirWhatsApp('${produto.nome}')">
                    <div class="marquee-image-wrapper">
                        <img src="${produto.imagem}" alt="${produto.nome}" class="marquee-image" loading="lazy">
                    </div>
                    <div class="marquee-content">
                        <div class="marquee-product-name">${produto.nome}</div>
                        <div class="marquee-product-copy">${produto.copy}</div>
                    </div>
                </div>
            `;
        }

        function renderizarMarquees() {
            // Renderizar marquee do hero
            const heroMarquee = document.getElementById('heroMarquee');
            if (heroMarquee) {
                const htmlHero = produtosHeroMarquee.map(img => `
                    <div class="hero-marquee-item">
                        <img src="produtos site 2/Maque hero/${img}" alt="Produto Banca Pacheco" loading="lazy">
                    </div>
                `).join('');
                heroMarquee.innerHTML = htmlHero + htmlHero; // Duplica para loop infinito
            }

            // Renderizar produtos sem duplicar (scroll manual)
            const marqueeQueijosArtesanais = document.getElementById('marqueeQueijosArtesanais');
            const htmlQueijosArtesanais = produtosQueijosArtesanais.map(criarMarqueeItem).join('');
            marqueeQueijosArtesanais.innerHTML = htmlQueijosArtesanais;

            const marqueeQueijosCatarinense = document.getElementById('marqueeQueijosCatarinense');
            const htmlQueijosCatarinense = produtosQueijosCatarinense.map(criarMarqueeItem).join('');
            marqueeQueijosCatarinense.innerHTML = htmlQueijosCatarinense;

            const marqueeQueijosImportados = document.getElementById('marqueeQueijosImportados');
            const htmlQueijosImportados = produtosQueijosImportados.map(criarMarqueeItem).join('');
            marqueeQueijosImportados.innerHTML = htmlQueijosImportados;

            const marqueeQueijosNacionais = document.getElementById('marqueeQueijosNacionais');
            const htmlQueijosNacionais = produtosQueijosNacionais.map(criarMarqueeItem).join('');
            marqueeQueijosNacionais.innerHTML = htmlQueijosNacionais;

            const marqueeFrios = document.getElementById('marqueeFrios');
            const htmlFrios = produtosFrios.map(criarMarqueeItem).join('');
            marqueeFrios.innerHTML = htmlFrios;
        }

        // Fechar modal ao clicar fora
        window.onclick = function(event) {
            const modalLojas = document.getElementById('modalLojas');
            if (event.target === modalLojas) {
                fecharModal('modalLojas');
            }
        }
